module KibanaConfig

  es_ip = "<%= @es_ip %>"
  es_port = "<%= @es_port %>"
  Elasticsearch = "<%= @es_ip %>:<%= @es_port %>"
  KibanaPort = "<%= @kibana_port %>"
  KibanaHost = ENV['KIBANA_HOST']

<% KibanaConf.config_to_human(@config).each do |key, value| -%>
  <%= key+' = '+value %>
<% end -%>

end
